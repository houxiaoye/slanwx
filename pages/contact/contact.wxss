/* pages/contact/contact.wxss */
page{
  padding:0 30rpx 0 30rpx;
  box-sizing: border-box;
  background: url('https://mmbiz.qpic.cn/mmbiz_png/PWLmhkicnSZQs0Kf9ePYlL84Ja3du5UIeZXiaTrOSr0v3g0pUK0PHXKL0ENSxlwBoy3yTv7Jyic6NCok32UgFByGw/0?wx_fmt=png') no-repeat;  
  background-size:100% 100%;
  display:flex;
  align-items:center;
  justify-content: center;
  flex-wrap:wrap;
}
.logo {
  width:100%;
  height:240rpx;
  display: flex;
  justify-content: center;
  align-items:center;
}
.logo image{
  width:240rpx;
  height:48rpx;
}
.content{
  width:560rpx;
   height:480rpx; 
   /* padding-top:45rpx;
  padding-left:64rpx; */
  padding:45rpx 0 0 64rpx;
  box-sizing:border-box;
  color:#00aeff;
  font-size:22rpx;
  background: url('https://mmbiz.qpic.cn/mmbiz_png/PWLmhkicnSZTl0u7UTiaA8rnSg10wnuV8gHW2xeLbiczic0EsibQC9pZaB23PVeNlduFgf8ib4k52wtQ5qlcWBaiaYhDQ/0?wx_fmt=png') no-repeat;
  background-size:100% 100%;
  /* display:flex;
  justify-content: center;
  align-items:center; */
}
.content .centerborder{
    display: inline-block;
    width:432rpx;
    height: 34rpx;
    margin:10rpx 0 10rpx 0;
}
.content .top image,
.content .bottom image{
    width:45rpx;
    height:45rpx;
    margin-right:12rpx;
}
.content .top text,
.content .bottom text{
  display:block;
  font-size:28rpx;
  margin-bottom:30rpx;
}
.content .top view,
.content .bottom view{
  display:flex;
  /* justify-content:center; */
  align-items:center;
  margin:10rpx 0;
}
.content .tel{
  margin-top:50rpx;
}
