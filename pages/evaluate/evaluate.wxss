page{
width: 100%;
  height: 100%;
  padding:0 30rpx 0 30rpx;
  box-sizing: border-box;
  background: url('https://mmbiz.qpic.cn/mmbiz_png/PWLmhkicnSZQs0Kf9ePYlL84Ja3du5UIeZXiaTrOSr0v3g0pUK0PHXKL0ENSxlwBoy3yTv7Jyic6NCok32UgFByGw/0?wx_fmt=png') no-repeat;  
  background-size:100% 100%;
}
.evaluatetitle{
	margin: 0 0 60rpx 30rpx;
	padding-top:91rpx;
	box-sizing: border-box;
	color:#00a2ff;
	font-size: 32rpx;
}
.emoticom{
	width: 100%;
	padding-bottom: 56rpx;
	padding-left: 30rpx;
	box-sizing: border-box;
	display: flex;
	justify-content: space-between;
	border-bottom: 1rpx solid #122f52;
}
.emoticom view{
	width: 168rpx;
	height: 168rpx;
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	align-items: center;
	border:2rpx solid #00a2ff;
	border-radius:8rpx;
	text-align:center;
	color:#00a2ff;
}
.emoticom image{
	margin-top: 20rpx;
	width: 83rpx;
	height: 83rpx;
}
.emoticom text{
	width: 100%;
	display: block;
  font-size:32rpx;
}
.agrees{
	background: #00a2ff;
	color: #fff;
}
.advice{
	width: 100%;
	padding-left: 30rpx;
	box-sizing: border-box;
}
.advice .rebacktitle{
  font-size:32rpx;
}
.advice text{
	/*margin-top: 42rpx;*/
	/*margin-left: 42rpx;*/
	/*margin-bottom: 43rpx;*/
	color:#00a2ff;
	font-size: 26rpx;
}
.advice textarea{
	width: 634rpx;
	height: 300rpx;
	margin-top: 43rpx;
	padding:30rpx;
	color: #00a2ff;
	box-sizing: border-box;
	border:1rpx solid #122b67;
	font-size: 26rpx;
}
.advice form button{
	width: 500rpx;
	height: 100rpx;
	margin:52rpx 0 0 55rpx;
	color: #fff;
	background: #00a2ff;
}
.advice .forms {
	position: relative;
}
.advice .tconer,
.advice .bconer,
.st{
	position: absolute;
	width: 21rpx;
	height: 19rpx;
	display: block;
}
.advice .tconer{
	top:0;
}
.advice .bconer{
	bottom:0rpx;
	right:26rpx;
}
.stbox{
	position: absolute;
}
.bconerst4{
	bottom:0;
	left:0;
}
.bconerst1{
	top:0;
	right:26rpx;
}
.st1{
	right:-10rpx;
	top: 40rpx;
}
.st2{
	right:-9rpx;
	bottom: -12rpx;
}
.st3{
	top:40rpx;
	left:45rpx;
}
.st4{
	left:45rpx;
	bottom:-12rpx;
}
